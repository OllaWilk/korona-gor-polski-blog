{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "eslint:eslint-config-airbnb",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "indent": ["error", 2],
    "linebreak-style": ["off"],
    "quotes": ["error", "single", { "allowTemplateLiterals": true }],
    "semi": ["error", "always"],
    "no-console": ["off"]
  }
}
